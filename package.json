{
  "name": "japan-map",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/react-router": "^5.1.20",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.8.1",
    "react-scripts": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@types/babel__core": "^7.20.0",
    "@types/react-dom": "^18.0.11",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "containers": "docker ps -a",
    "web-build": "docker build . -t japan-map-image:latest",
    "web-run": "docker run -it --rm -d -p 8080:80/tcp --name japan-map japan-map-image",
    "web-stop": "docker rm -f japan-map || true",
    "web-start": "npm run web-build && npm run web-run",
    "dev": "npm run web-stop && npm run web-start"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
